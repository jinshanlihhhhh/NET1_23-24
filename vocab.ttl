@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix o: <http://omeka.org/s/vocabs/o#> .
@prefix mud: <https://jardindesconnaissances.univ-paris8.fr/onto/mud#> .
@prefix teach: <http://linkedscience.org/teach/ns#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix schema: <http://schema.org/>.

<https://jardindesconnaissances.univ-paris8.fr/onto/mud> a owl:Ontology ;
    dcterms:title "musee digital" .

mud:Departement a rdfs:Class ;
    rdfs:label "Département" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

mud:University a rdfs:Class ;
    rdfs:label "Université" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

mud:UFR a rdfs:Class ;
    rdfs:label "Unité de Formation et de Recherche" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

mud:Mention a rdfs:Class ;
    rdfs:label "Mention" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

mud:SalleCours a rdfs:Class ;
    rdfs:label "Salle de cours" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .    

mud:Parcours a rdfs:Class ;
    rdfs:label "Parcours" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

mud:UE a rdfs:Class ;
    rdfs:label "Unité d'enseignement" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

mud:EC a rdfs:Class ;
    rdfs:label "Élément constitutif" ;
    rdfs:domain o:Resource ;
    owl:sameAs schema:Course ;    
    vs:term_status "experimental" .


mud:pcCM a rdf:Property ;
    rdfs:label "% Cours magistraux" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

mud:pcTD a rdf:Property ;
    rdfs:label "% Travaux dirigés" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

mud:pcTP a rdf:Property ;
    rdfs:label "% Travaux pratiques" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

mud:sd a rdf:Property ;
    rdfs:label "Seuil dédoublement" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

mud:libre a rdf:Property ;
    rdfs:label "EC Libre" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

